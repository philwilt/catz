{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "react/prefer-stateless-function": [1, { "ignorePureComponents": true }]
  }
}
